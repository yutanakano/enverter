{
    "coc.preferences.extensions.autoInstall": [
        "coc-explorer",
        "coc-tsserver",
        "coc-prettier",
        "coc-eslint"
    ],
    "explorer.icon.enableNerdfont": true,
    "explorer.icon.enableDevicons": true,
    "explorer.icon.enableUnicode": true,
    "explorer.icon.enableASCII": true,
    "explorer.git.enable": true,
    "explorer.git.icon.opened": "",
    "explorer.git.icon.modified": "",
    "explorer.git.icon.added": "",
    "explorer.git.icon.deleted": "",
    "explorer.git.icon.renamed": "",
    "explorer.git.icon.copied": "",
    "explorer.git.icon.ignored": "◌",
    "explorer.openAction.strategy": "custom",
    "explorer.openAction.custom": "edit",
    "explorer.openAction.follow": true,
    "explorer.confirmDelete": true,
    "explorer.copy.showConfirmationMessage": true,
    "explorer.delete.showConfirmationMessage": true,
    "explorer.preview.preserveWindowLayout": true,
    "explorer.preview.maxWindowSize": 20,
    "explorer.file.reveal.whenOpen": true,
    "explorer.autoRefresh": true,
    "explorer.autoExpandMaxDepth": 3,
    "explorer.previewAction.onHover": true,
    "explorer.fuzzyMatch": true,
    "explorer.ignorePatterns": ["node_modules", ".git", ".DS_Store"],
    "explorer.file.showHiddenFiles": true,
    "explorer.git.icon.status.ignored": "◌"
}
